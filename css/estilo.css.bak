*{
margin:0;
padding:0;
font:18px ource Sans Pro, sans-serif;

}
html {
      scroll-behavior: smooth;
}
/* width */
::-webkit-scrollbar {
  width: 10px;
}

/* Track */
::-webkit-scrollbar-track {
  background: #00621A; 
}
 
/* Handle */
::-webkit-scrollbar-thumb {
  background: #00FF43; 
}

/* Handle on hover */
::-webkit-scrollbar-thumb:hover {
  background: #00FF43; 
}
body{
	width:97%;
	height:85vh;
	background-color:#222021;
}
header{
	background-color:#151819;
	width:100%;
	height:12vh;
	padding-left:3%;
	padding-bottom:1%;
	padding-top:1%;
	position:fixed;
	top:0;
	z-index:10;
}
a{
	text-decoration:none;
	cursor:pointer;
	color: #616d72;
}
#logo{
	width:15%;
	height:100%;
	display:inline-block;
	margin-right:16%;
}
img{
	width:100%;
	height:100%;
}
.menu{
	width:auto;
	height:auto;
	display:inline-block;
	color: #616d72;
	margin-left:2%;
}
.a-menu:hover{
	color:#00FF43;
}
section{
	position:relative;
	width:103%;
	height:auto;
	top:20vh;
}

.conteudo {
	display: flex;
	justify-content: center;
	align-items: center;
}

.div-conteudo {
	width: 50%;
	height: auto;
}

h1 {
	width:100%;
	height:auto;
	color: #00FF43;
	font-size: 70px;
	text-align: center;
	padding-top: 2%;
	font-weight: bold;
	margin: 0; /* Remova o espaço superior do h1 */
}

.texto-section {
	color: white;
	text-align: center;
	padding-left: 2%;
	padding-right: 2%;
	padding-top: 3%;
	margin: 0; /* Remova o espaço superior do texto */
}

.conteudo-img {
	width: 49%;
}
table{
	width:100%;
	height:auto;
	background-color:white;
	color:#00FF43;
	text-align:center;
    border-collapse: collapse;
}
tr{
	margin-bottom:5px;
}







footer{
	position:relative;
	margin-top:100px;
}